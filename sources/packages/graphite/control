#!/bin/bash

NAME="graphite"
VERSION=0.9.8
RELEASE=0
DESCRIPTION=""
REQUIRES="canohome pkgmgr python"

function install(){
	echo "Install $NAME $VERSION-$RELEASE ..."
	check_code $?
}

function remove(){
	echo "Remove $NAME $VERSION-$RELEASE ..."
	check_code $?
}

function purge(){
	echo "Purge $NAME $VERSION-$RELEASE ..."
	check_code $?
}

function update(){
	echo "Update $NAME $VERSION-$RELEASE ..."
	check_code $?
}
