#!/bin/bash

NAME="rabbitmq-server"
VERSION=2.4.1
RELEASE=0
DESCRIPTION=""
REQUIRES="canohome pkgmgr erlang canotools"

function install(){
	echo "Install $NAME $VERSION-$RELEASE ..."
	check_code $?
}

function remove(){
	echo "Remove $NAME $VERSION-$RELEASE ..."
	check_code $?
}

function purge(){
	echo "Purge $NAME $VERSION-$RELEASE ..."
	check_code $?
}

function update(){
	echo "Update $NAME $VERSION-$RELEASE ..."
	check_code $?
}
