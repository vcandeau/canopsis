NAME="wkhtmltopdf"

FCHECK="$PREFIX/bin/wkhtmltopdf"

BASE="wkhtmltopdf-$VERSION-static-$ARCH"
LOG="$LOG_PATH/$BASE.log"
rm -f $LOG &> /dev/null


function build(){
	true
}

function install(){

	cd $PREFIX/bin
	tar xfj $SRC_PATH/externals/$BASE.tar.bz2

	ln -s wkhtmltopdf-i386 wkhtmltopdf &> /dev/null
	ln -s wkhtmltopdf-amd64 wkhtmltopdf &> /dev/null
	
	install_basic_source $NAME
}
