NAME="pyperfstore"

LOG="$LOG_PATH/$NAME.log"
rm -f $LOG &> /dev/null

function build(){
	true
}

function install(){
	rm -R $PREFIX/lib/python2.7/site-packages/pyperfstore* 1>> $LOG 2>> $LOG

	cd $SRC_PATH/$NAME

	$PY_BIN setup.py build 1>> $LOG 2>> $LOG
	$PY_BIN setup.py install --prefix=$PREFIX 1>> $LOG 2>> $LOG
	check_code $? "Setup.py install failure"	
}
